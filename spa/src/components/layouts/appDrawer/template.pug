//- Slide-out navigation drawer
v-navigation-drawer(
  app
  disable-resize-watcher
  v-model = "position"
  temporary
)
  //- List of page components comes from routes
  v-list(shaped)
    v-list-item(
      v-for="route in routes"
      :key="route.name"
      :to="route.url"
    )
      v-list-item-icon
        v-icon {{ route.icon }}
      v-list-item-title {{ route.name }}
        



    