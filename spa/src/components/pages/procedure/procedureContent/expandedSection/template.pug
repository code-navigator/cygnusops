v-expansion-panels(
  multiple
  accordion
)
  //- Loop through all sections, creating an expansion panel for each one
  v-expansion-panel(
    v-for="(section, i) in sections"
    v-show="section.content"
    :key="i"
  )
    //- Title is always visible
    v-expansion-panel-header.title(
      v-html = "section.title"
    )
    //- Content is collapsible
    v-expansion-panel-content
      //- v-card-text( 
      //-   v-html="section.content"
      //- )
      //- router-link(to="/procedures/procedure-102") Click
      templateCompiler(content="<router-link to='/procedures/procedure-102'>CLICK</router-link>")